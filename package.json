{
  "name": "nice-index",
  "displayName": "nice-index",
  "publisher": "skoshy",
  "description": "Cleaner tab titles for index.* files",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
